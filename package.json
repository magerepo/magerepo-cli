{
	"name": "magerepo-cli",
	"version": "2.0.2",
	"description": "Cross-platform CLI tool to interact with the MageRepo API",
	"main": "src/index.js",
	"repository": "git@github.com:magerepo/magerepo-cli.git",
	"author": "Rafael Grigorian <development@jetrails.com>",
	"license": "MIT",
	"private": true,
